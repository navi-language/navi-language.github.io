{
  "$schema": "https://json.schemastore.org/tmlanguage.json",
  "$examples": [
    "https://github.com/microsoft/vscode/blob/main/extensions/typescript-basics/syntaxes/TypeScript.tmLanguage.json",
    "https://github.com/microsoft/vscode/blob/main/extensions/ruby/syntaxes/ruby.tmLanguage.json"
  ],
  "name": "Navi",
  "scopeName": "source.navi",
  "patterns": [
    {
      "include": "#comments"
    },
    {
      "include": "#comments"
    },
    {
      "comment": "Interpreted string literals",
      "begin": "\"",
      "beginCaptures": {
        "0": {
          "name": "punctuation.definition.string.begin.navi"
        }
      },
      "end": "\"",
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.string.end.navi"
        }
      },
      "name": "string.quoted.double.navi",
      "patterns": [
        {
          "include": "#string_escaped_char"
        },
        {
          "include": "#string_placeholder"
        }
      ]
    },
    {
      "comment": "String Interpolation",
      "begin": "`",
      "beginCaptures": {
        "0": {
          "name": "punctuation.definition.string.begin.navi"
        }
      },
      "end": "`",
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.string.end.navi"
        }
      },
      "name": "string.template.navi",
      "patterns": [
        {
          "include": "#interpolated_navi"
        },
        {
          "include": "#string_placeholder"
        }
      ]
    },
    {
      "include": "#numeric_literals"
    },
    {
      "comment": "Language constants",
      "match": "\\b(true|false|nil)\\b",
      "name": "constant.language.navi"
    },
    {
      "comment": "Terminators",
      "match": ";",
      "name": "punctuation.terminator.navi"
    },
    {
      "include": "#brackets"
    },
    {
      "include": "#delimiters"
    },
    {
      "include": "#keywords"
    },
    {
      "include": "#operators"
    }
  ],
  "repository": {
    "brackets": {
      "patterns": [
        {
          "begin": "{",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.begin.bracket.curly.navi"
            }
          },
          "end": "}",
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.end.bracket.curly.navi"
            }
          },
          "patterns": [
            {
              "include": "$self"
            }
          ]
        },
        {
          "begin": "\\(",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.begin.bracket.round.navi"
            }
          },
          "end": "\\)",
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.end.bracket.round.navi"
            }
          },
          "patterns": [
            {
              "include": "$self"
            }
          ]
        },
        {
          "match": "\\[|\\]",
          "name": "punctuation.definition.bracket.square.navi"
        }
      ]
    },
    "comments": {
      "patterns": [
        {
          "name": "comment.block.navi",
          "begin": "(\\/\\*)",
          "beginCaptures": {
            "1": {
              "name": "punctuation.definition.comment.navi"
            }
          },
          "end": "(\\*\\/)",
          "endCaptures": {
            "1": {
              "name": "punctuation.definition.comment.navi"
            }
          }
        },
        {
          "name": "comment.line.double-slash.navi",
          "begin": "(\\/\\/)",
          "beginCaptures": {
            "1": {
              "name": "punctuation.definition.comment.navi"
            }
          },
          "end": "(?:\\n|$)"
        }
      ]
    },
    "delimiters": {
      "patterns": [
        {
          "match": ",",
          "name": "punctuation.other.comma.navi"
        },
        {
          "match": "\\.(?!\\.\\.)",
          "name": "punctuation.other.period.navi"
        },
        {
          "match": ":(?!=)",
          "name": "punctuation.other.colon.navi"
        }
      ]
    },
    "keywords": {
      "patterns": [
        {
          "comment": "Flow control keywords",
          "match": "\\b(break|case|continue|default|else|for|in|while|if|return|switch)\\b",
          "name": "keyword.control.navi"
        },
        {
          "comment": "Define keywords",
          "match": "\\b(fn|use|let|struct|int|number|string|bool|tests|test|bench|impl|interface)\\b",
          "name": "keyword.define.navi"
        },
        {
          "comment": "Built-in libraries",
          "match": "\\b(std)\\b",
          "name": "keyword.define.navi"
        },
        {
          "comment": "Built-in functions",
          "match": "\\b(print|println|assert|assert_eq|assert_ne|trace|try)\\b",
          "name": "support.function.core.navi"
        }
      ]
    },
    "operators": {
      "comment": "Note that the order here is very important!",
      "patterns": [
        {
          "match": "(\\*|&)(?=\\w)",
          "name": "keyword.operator.address.navi"
        },
        {
          "match": "<\\-",
          "name": "keyword.operator.channel.navi"
        },
        {
          "match": "\\-\\-",
          "name": "keyword.operator.decrement.navi"
        },
        {
          "match": "\\+\\+",
          "name": "keyword.operator.increment.navi"
        },
        {
          "match": "(==|!=|<=|>=|<(?!<)|>(?!>))",
          "name": "keyword.operator.comparison.navi"
        },
        {
          "match": "(&&|\\|\\||!)",
          "name": "keyword.operator.logical.navi"
        },
        {
          "match": "(=|\\+=|\\-=|\\|=|\\^=|\\*=|/=|:=|%=|<<=|>>=|&\\^=|&=)",
          "name": "keyword.operator.assignment.navi"
        },
        {
          "match": "(\\+|\\-|\\*|/|%)",
          "name": "keyword.operator.arithmetic.navi"
        },
        {
          "match": "(&(?!\\^)|\\||\\^|&\\^|<<|>>)",
          "name": "keyword.operator.arithmetic.bitwise.navi"
        },
        {
          "match": "\\.\\.\\.",
          "name": "keyword.operator.ellipsis.navi"
        }
      ]
    },
    "string_escaped_char": {
      "patterns": [
        {
          "match": "\\\\([0-7]{3}|[abfnrtv\\\\'\"]|x[0-9a-fA-F]{2}|u[0-9a-fA-F]{4}|U[0-9a-fA-F]{8})",
          "name": "constant.character.escape.navi"
        },
        {
          "match": "\\\\[^0-7xuUabfnrtv\\'\"]",
          "name": "invalid.illegal.unknown-escape.navi"
        }
      ]
    },
    "string_placeholder": {
      "patterns": [
        {
          "match": "%(\\[\\d+\\])?([\\+#\\-0\\x20]{,2}((\\d+|\\*)?(\\.?(\\d+|\\*|(\\[\\d+\\])\\*?)?(\\[\\d+\\])?)?))?[vT%tbcdoqxXUbeEfFgGspw]",
          "name": "constant.other.placeholder.navi"
        }
      ]
    },
    "interpolated_navi": {
      "patterns": [
        {
          "begin": "\\$\\{",
          "beginCaptures": {
            "0": {
              "name": "punctuation.section.embedded.begin.navi"
            }
          },
          "contentName": "source.navi",
          "end": "\\}",
          "endCaptures": {
            "0": {
              "name": "punctuation.section.embedded.end.navi"
            },
            "1": {
              "name": "source.navi"
            }
          },
          "name": "meta.embedded.line.navi",
          "patterns": [
            {
              "include": "$self"
            }
          ]
        }
      ]
    },
    "variables": {
      "patterns": [
        {
          "match": "((\\w+)([\\s]*:[=]?)[\\s]*)",
          "captures": {
            "1": {
              "patterns": [
                {
                  "match": "\\d\\w*",
                  "name": "invalid.illegal.identifier.navi"
                },
                {
                  "include": "#delimiters"
                }
              ]
            }
          }
        },
        {
          "match": "(\\w+(?:,\\s*\\w+)*)(\\s+(\\[(\\d*|\\.\\.\\.)\\])*\\*?(<-)?\\w+(?:\\.\\w+)?\\s*[^=].*)",
          "captures": {
            "1": {
              "patterns": [
                {
                  "match": "\\d\\w*",
                  "name": "invalid.illegal.identifier.navi"
                },
                {
                  "match": "\\w+",
                  "name": "variable.other.declaration.navi"
                },
                {
                  "include": "#delimiters"
                }
              ]
            },
            "2": {
              "patterns": [
                {
                  "include": "$self"
                }
              ]
            }
          }
        },
        {
          "begin": "\\(",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.variables.begin.bracket.round.navi"
            }
          },
          "end": "\\)",
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.variables.end.bracket.round.navi"
            }
          },
          "patterns": [
            {
              "include": "$self"
            },
            {
              "include": "#variables"
            }
          ]
        }
      ]
    },
